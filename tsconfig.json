{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@components": ["src/components"],
      "@styles/*": ["src/styles/*"],
      "@styles": ["src/styles"],
      "@public/*": ["public/*"],
      "@public": ["public"],
      "@services/*": ["src/services/*"],
      "@services": ["src/services"],
      "@graphqlTypes/*": ["__generated__/*"],
      "@graphqlTypes": ["__generated__"]
    },
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "src/components/ecosystem/index.jsx",
    "pages/vide-upload/index.jsx",
    "src/components/video_upload/LayerBlur.jsx",
    "src/components/video_upload/index.jsx",
    "pages/index.jsx"
, "pages/events/socials.jsx", "src/components/components/ui/button.jsx", "src/components/components/ui/button.jsx"  ],
  "exclude": ["node_modules", "__generated__/globalTypes.ts"]
}
